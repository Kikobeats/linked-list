{
  "name": "linked-list",
  "version": "0.1.0",
  "description": "Minimalistic linked lists",
  "license": "MIT",
  "keywords": [
    "linked",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wooorm/linked-list.git"
  },
  "author": "Titus Wormer <tituswormer@gmail.com>",
  "devDependencies": {
    "has": "^1.0.1",
    "istanbul": "^0.3.0",
    "mocha": "^2.0.0",
    "uglify-js": "^2.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "test-api": "node_modules/.bin/_mocha --reporter spec --check-leaks -u exports test.js",
    "test-travis": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --reporter spec --check-leaks -u exports test.js",
    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -- test.js",
    "lint": "xo",
    "test": "npm run link && npm run coverage"
  },
  "xo": {
    "space": true
  }
}
